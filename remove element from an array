public class Main
{
	public static void main(String[] args) {
		int arr[]={10,20,30,60,40,50};
	    int del=60;
		for(int i=0;i<arr.length;i++)
		{
		    if(arr[i]==del)
		    {
		        for(int j=i;j<arr.length;j++)
		        {
		            arr[j]=arr[j+1];
		        }
		    }
		    break;

		}
		for(int k=0;k<arr.length;k++)
		{
		    System.out.println(arr[k]);
		}
	}
}
