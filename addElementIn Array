public class Main
{
	public static void main(String[] args) {
		int arr[]={1,2,3,4,5,6};
		int newArr[]=new int[arr.length+1];
	    int index=4;
	    int j=0;
	    for(int i=0;i<newArr.length;i++)
	    {
	        if (i==index){
	            newArr[i]=9;
	        }
	        else{
	            newArr[i]=arr[j];
	            j++;
	        }
	    }
	    for( int k=0;k<newArr.length;k++)
	    {
	        System.out.println(newArr[k]);
	    }
	}
}
